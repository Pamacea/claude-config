{
  "env": {
    "ANTHROPIC_AUTH_TOKEN": "xxxx",
    "ANTHROPIC_BASE_URL": "xxxx",
    "API_TIMEOUT_MS": "3000000",
    "CLAUDE_CODE_DISABLE_NONESSENTIAL_TRAFFIC": "1",
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "statusLine": {
    "type": "command",
    "command": "node C:/Users/NOM/.claude/statusline.mjs"
  },
  "enabledPlugins": {
    "git-master@claude-git": true,
    "claude-mem@thedotmack": true,
    "agents@smite": true,
    "core@smite": true,
    "essentials@smite": true,
    "studio@smite": true,
    "ui@strike": true
  },
  "skipDangerousModePermissionPrompt": true,
  "teammateMode": "tmux",
  "hooks": {
    "SessionStart": [
      {
        "matcher": "startup",
        "hooks": [
          {
            "type": "command",
            "command": "node C:/Users/NOM/.claude/hooks/session-start.cjs"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node C:/Users/NOM/.claude/hooks/prompt-check.cjs"
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "node C:/Users/NOM/.claude/hooks/bash-validate.cjs"
          }
        ]
      },
      {
        "matcher": "Grep",
        "hooks": [
          {
            "type": "command",
            "command": "node C:/Users/NOM/.claude/hooks/grep-validate.cjs"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "TaskCreate|TaskUpdate",
        "hooks": [
          {
            "type": "command",
            "command": "node C:/Users/NOM/.claude/hooks/task-progress.cjs"
          }
        ]
      }
    ]
  }
}
